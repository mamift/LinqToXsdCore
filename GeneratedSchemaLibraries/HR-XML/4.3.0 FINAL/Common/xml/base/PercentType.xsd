<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hropen="https://hropenstandards.org/schema/xml/" targetNamespace="https://hropenstandards.org/schema/xml/" version="4.2.0" id="PercentType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name="PercentType">
    <xsd:annotation>
      <xsd:documentation>A percent is a value representing a fraction of one hundred, expressed as a quotient.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal" />
  </xsd:simpleType>
  <xsd:element name="Percent" type="hropen:PercentType" />
</xsd:schema>