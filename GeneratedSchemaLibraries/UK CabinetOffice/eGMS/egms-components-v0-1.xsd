				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ReviewDetails</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ReviewerDetails</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ScheduleId</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">TimePeriod</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">IsDefinedBy</ProvidesDefinitionOf>
