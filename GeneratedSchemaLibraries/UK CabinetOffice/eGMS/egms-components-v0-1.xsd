				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ProtectiveMarking</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ProtectiveMarkingChnageDate</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ProtectiveMarkingExpiryDate</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ReleaseStatusType</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">ReleaseStatus</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">StatusDescription</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">Version</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">Category</ProvidesDefinitionOf>
				<ProvidesDefinitionOf Namespace="http://www.govtalk.gov.uk/metadata/egms">Keyword</ProvidesDefinitionOf>
