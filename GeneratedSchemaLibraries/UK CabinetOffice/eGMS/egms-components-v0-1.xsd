	<xs:import namespace="urn:oasis:names:tc:ubl:codelist:LanguageIdentificationCode:1:0" schemaLocation="UBL-CodeList-LanguageIdentificationCode-1.0.xsd"/>
	<!-- ********************************* -->
	<!-- General e-GMS components -->
	<!-- ********************************* -->
	<xs:attributeGroup name="RoleGroup">
		<xs:attribute name="Role" type="xs:token"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="HrefGroup">
		<xs:attribute name="Href" type="xs:anyURI"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="NamespaceGroup">
		<xs:attribute name="Namespace" type="xs:anyURI"/>
	</xs:attributeGroup>
	<xs:attribute name="Refines" type="xs:NMTOKEN"/>
	<xs:attributeGroup name="SchemeGroup">
		<xs:attribute name="Scheme" type="xs:anyURI"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="UBLcodeGroup">
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="codeListID" type="xs:normalizedString" use="optional"/>
		<xs:attribute name="codeListAgencyName" type="xs:string" use="optional"/>
		<xs:attribute name="codeListName" type="xs:string" use="optional"/>
		<xs:attribute name="codeListVersionID" type="xs:normalizedString" use="optional"/>
		<xs:attribute name="codeListURI" type="xs:anyURI" use="optional"/>
		<xs:attribute name="codeListSchemeURI" type="xs:anyURI" use="optional"/>
		<xs:attribute name="languageID" type="xs:language" use="optional" fixed="en"/>
	</xs:attributeGroup>
	<xs:complexType name="PeriodStructure">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="Uom" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:enumeration value="days"/>
							<xs:enumeration value="weeks"/>
							<xs:enumeration value="months"/>
							<xs:enumeration value="years"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="PersonOrOrganisationType">
		<xs:simpleContent>
			<xs:extension base="xs:token">
				<xs:attributeGroup ref="HrefGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- sub-elements of temporal  and Valid-->
	<xs:element name="EndDate" type="xs:date"/>
	<xs:element name="StartDate">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date"/>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<!--end of  sub-elements of temporal  and Valid-->
	<!-- ******************** -->
	<!-- The DC elements -->
	<!-- ******************** -->
	<xs:element name="Contributor">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attributeGroup ref="RoleGroup"/>
					<xs:attributeGroup ref="HrefGroup"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Creator">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attributeGroup ref="HrefGroup"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Description" type="xs:string"/>
	<xs:element name="Format">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attributeGroup ref="SchemeGroup"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Identifier">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attributeGroup ref="HrefGroup"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Language" type="iso639:LanguageIdentificationCodeType"/>
	<xs:element name="Publisher" type="PersonOrOrganisationType"/>
	<xs:element name="Title" type="xs:string"/>
	<xs:element name="Type" type="xs:token"/>
	<!-- ******************************** -->
	<!-- Additional e-GMS elements -->
	<!-- ******************************** -->
	<xs:element name="Status">
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="approved"/>
				<xs:enumeration value="deprecated"/>
				<xs:enumeration value="draft"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- *********************** -->
	<!-- The DC refinements -->
	<!-- *********************** -->
	<!-- refinements of Date -->
	<xs:element name="Created">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Copyrighted">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Modified">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Valid">
		<xs:annotation>
			<xs:documentation>used</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="StartDate"/>
					<xs:element ref="EndDate" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="EndDate"/>
			</xs:choice>
			<xs:attribute ref="Refines" fixed="Date"/>
		</xs:complexType>
	</xs:element>
	<!--end of refinements of Date -->
	<!--refinements of Relation-->
	<xs:element name="ConformsTo">
		<xs:annotation>
			<xs:documentation>For the XML Schema profile, expect one instance to have a value of http://www.w3.org/2001/XMLSchema</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HasFormat">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HasVersion">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HasPart">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="IsPartOf">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="IsReplacedBy">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="IsVersionOf">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Requires">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Replaces">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute ref="Refines" use="optional" fixed="Relation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<!--end of refinements of Relation-->
	<!--refinements of Coverage-->
	<xs:element name="Spatial">
		<xs:annotation>
			<xs:documentation>The Scheme attribute allows use of languages such as GML to define the metadata value</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute ref="Refines" fixed="Coverage"/>
			<xs:attribute name="Scheme"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Temporal">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="StartDate"/>
					<xs:element ref="EndDate" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="EndDate"/>
			</xs:choice>
			<xs:attribute ref="Refines" fixed="Coverage"/>
		</xs:complexType>
	</xs:element>
	<!--end of refinements of Coverage-->
	<!-- *********************************** -->
	<!-- Additional e-GMS refinements -->
	<!-- *********************************** -->
	<!--refinements of Date -->
	<xs:element name="Issued">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NextVersionDue">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="UpdatingFrequency">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="PeriodStructure">
					<xs:attribute ref="Refines" fixed="Date"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<!--end of refinements of Date -->
	<!--refinements of Disposal-->
	<xs:simpleType name="ActionType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="archive"/>
			<xs:enumeration value="deprecate"/>
			<xs:enumeration value="remove"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Action">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="ActionType">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AuthorisedBy">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="PersonOrOrganisationType">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Comment">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Conditions">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DisposalDate">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LastReview">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExportDestination">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attributeGroup ref="HrefGroup"/>
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Review">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:date">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReviewDetails">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReviewerDetails">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="PersonOrOrganisationType">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ScheduleId">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attribute ref="Refines" fixed="Disposal"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TimePeriod">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="PeriodStructure">
					<xs:attribute ref="Refines" fixed="Disposal"/>
