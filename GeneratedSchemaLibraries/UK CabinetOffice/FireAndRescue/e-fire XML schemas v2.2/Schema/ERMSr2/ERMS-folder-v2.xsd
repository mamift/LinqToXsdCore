<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://www.govtalk.gov.uk/CM/gms" xmlns="http://www.govtalk.gov.uk/CM/gms" xmlns:pdt="http://www.govtalk.gov.uk/people/PersonDescriptives" xmlns:bs7666="http://www.govtalk.gov.uk/people/bs7666" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:core="http://www.govtalk.gov.uk/core" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2" id="ERMS-folder">	<xsd:annotation>		<xsd:documentation>	v2: Namespaces and schema structure revised following GovTalk review. </xsd:documentation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Audience>e-records software suppliers</Audience>				<Audience>e-records integrators</Audience>				<Audience>archivists and records managers</Audience>				<Contributor>National Archives</Contributor>				<Contributor>Office of the e-Envoy</Contributor>				<Creator>Hedra Ltd http://www.hedra.com</Creator>				<Date>					<Created>2004-02-12</Created>				</Date>				<Date>					<Valid>						<BeginningDate>2004</BeginningDate>					</Valid>				</Date>				<Description>Schema to represent the 2003 local implementation of e-GMS2 in ERMS (Electronic Records Management System) metadata at folder level of aggregation</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.govtalk.gov.uk/Archives and records management}ERM schemas</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Relation>					<Requires>{http://www.govtalk.gov.uk/people/bs7666}</Requires>				</Relation>				<Relation>					<ProvidesDefinitionOf>ERMS folder level metadata</ProvidesDefinitionOf>				</Relation>				<Rights>unclassified</Rights>				<Rights>					<Copyright>Crown copyright 2004</Copyright>				</Rights>				<Subject>					<Category>Information management</Category>				</Subject>				<Subject>					<Project/>				</Subject>				<Title>ERMS folder level schema</Title>				<Type>record folder aggregation</Type>			</Metadata>		</xsd:appinfo>	</xsd:annotation>	<xsd:include schemaLocation="ERMS-general-subelements-v2.xsd"/>	<xsd:import namespace="http://www.govtalk.gov.uk/core" schemaLocation="../gdsc/CommonSimpleTypes-v1-3.xsd"/>	<xsd:import namespace="http://www.govtalk.gov.uk/people/PersonDescriptives" schemaLocation="../gdsc/PersonDescriptiveTypes-v1-0.xsd"/>	<xsd:import namespace="http://www.govtalk.gov.uk/people/bs7666" schemaLocation="../gdsc/BS7666-v1-3.xsd"/>	<xsd:element name="Metadata">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Aggregation"/>				<xsd:element ref="Date"/>				<xsd:element ref="Description" minOccurs="0" maxOccurs="unbounded"/>				<xsd:element ref="Disposal" maxOccurs="unbounded"/>				<xsd:element ref="Identifier"/>				<xsd:element ref="Location" minOccurs="0"/>				<xsd:element ref="Mandate" minOccurs="0" maxOccurs="unbounded"/>				<xsd:element ref="Relation" minOccurs="0"/>				<xsd:element ref="Rights" minOccurs="0" maxOccurs="unbounded"/>				<xsd:element ref="Subject" minOccurs="0" maxOccurs="unbounded"/>				<xsd:element ref="Title"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Aggregation">		<xsd:simpleType>			<xsd:annotation>				<xsd:documentation> This element states the aggregation level to which Folder metadata applies.</xsd:documentation>			</xsd:annotation>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="folder"/>				<xsd:enumeration value="marker (folder)"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Date">		<xsd:complexType>			<xsd:annotation>				<xsd:documentation> This type contains the Date structure used in Folder metadata.</xsd:documentation>			</xsd:annotation>			<xsd:sequence>				<xsd:element ref="Opened"/>				<xsd:element ref="Closed"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Identifier">		<xsd:complexType>			<xsd:annotation>				<xsd:documentation> This type contains the Identifier structure used in Folder metadata.</xsd:documentation>			</xsd:annotation>			<xsd:sequence>				<xsd:element ref="SystemID"/>				<xsd:element ref="FileplanID"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Relation">		<xsd:complexType>			<xsd:annotation>				<xsd:documentation> This type contains the Relation structure used in Folder metadata. The relationships between Folder and Class, Record and Folder, and Part and Folder, are stated in Class, Record and Part metadata respectively.</xsd:documentation>			</xsd:annotation>			<xsd:sequence>				<xsd:element ref="SeeAlso" minOccurs="0"/>				<xsd:element ref="HybridPaperLink" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element></xsd:schema>