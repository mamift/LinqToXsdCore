	<xs:complexType name="LocationStructure">
		<xs:annotation>
			<xs:documentation>The structure that should be used for specifying a particular location.  this allows the most flexibility with a BS7666 address structure with optional UPRN, and also allows optional coordinates to be recorded. There is a key attribute to qualify the validation performed against the address.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Address">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="bs7666:BSaddressStructure">
								<xs:sequence>
									<xs:element name="Coordinates" type="bs7666:CoordinateStructure" minOccurs="0"/>
								</xs:sequence>
								<xs:attribute name="Qualifier" type="apd:AddressQualifierType"/>
								<xs:attribute name="Validation" type="apd:AddressValidationType" use="required"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="NonAddressable">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Coordinates" type="bs7666:CoordinateStructure"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="OtherGazetteerContent" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ExternalReferences" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RCCgazetteerIdentifier" type="StringRestrictedMixedType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The unique identifier used by the RCC gazetteer system.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FRSgazetteerIdentifier" type="StringRestrictedMixedType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The unique identifier used by the FRS gazetteer system.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="TOID" type="StringRestrictedMixedType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Ordance Survey Topographic Identifier.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="OSAPR" type="StringRestrictedMixedType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Ordance Survey Address-Point Reference.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="OrganisationName" type="StringRestrictedMixedType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The Comapny/Business/Organisation Name attached to an address.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="AddressChanged" type="core:YesNoType" use="optional">
									<xs:annotation>
										<xs:documentation>The Address has been changed on the form that may mean that it is no longer consistent with the External References passed to the IRS system.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="OtherContent" type="ContextDataStructure" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="LocationDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Descriptive text for the Location e.g. In the front garden, On the pavement. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="NotKnownType">
		<xs:annotation>
			<xs:documentation>This type contains only the string enumeration value '0' to represent 'Not known'. This is expected to be used with the union option to add this option to other enumeration lists.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="YesNoDontKnowType">
		<xs:annotation>
