            <xsd:element minOccurs="0" name="artworkContentResponseStatus" type="artwork_content_common:ArtworkContentResponseStatusType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ArtworkContentDataCarrierDetailType">
