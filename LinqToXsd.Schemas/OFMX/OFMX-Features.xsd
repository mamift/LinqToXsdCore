        </xsd:annotation>
      </xsd:element>
      <xsd:element name="uomDistVerUpper" type="uomDistVer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Unit of measurement [upper limit]</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="codeDistVerLower" type="codeDistVer" minOccurs="0">
