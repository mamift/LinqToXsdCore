  <xsd:simpleType name="RatioType">
    <xsd:annotation>
      <xsd:documentation>Relation between two independent quantities, using the same unit.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal" />
  </xsd:simpleType>
  <xsd:element name="Ratio" type="hropen:RatioType" />
</xsd:schema>