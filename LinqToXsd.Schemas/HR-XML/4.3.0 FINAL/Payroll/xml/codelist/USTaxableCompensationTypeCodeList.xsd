      <xsd:enumeration value="MedicareTaxableCompensation" />
      <xsd:enumeration value="SocialSecurityTipsTaxableCompensation" />
      <xsd:enumeration value="StateTaxableCompensation" />
      <xsd:enumeration value="StateDisabilityInsuranceTaxableCompensation" />
      <xsd:enumeration value="StateUnemploymentInsuranceTaxableCompensation" />
