										<xsd:enumeration value="SI1"/>
										<xsd:enumeration value="SO1"/>
										<xsd:enumeration value="SP1"/>
										<xsd:enumeration value="SP2"/>
										<xsd:enumeration value="SP3"/>
										<xsd:enumeration value="SP4"/>
										<xsd:enumeration value="ST1"/>
										<xsd:enumeration value="SU1"/>
										<xsd:enumeration value="SU2"/>
										<xsd:enumeration value="SW1"/>
										<xsd:enumeration value="SW2"/>
										<xsd:enumeration value="TA1"/>
										<xsd:enumeration value="TH1"/>
										<xsd:enumeration value="TO1"/>
										<xsd:enumeration value="TO2"/>
										<xsd:enumeration value="TP1"/>
										<xsd:enumeration value="TP2"/>
										<xsd:enumeration value="TP3"/>
										<xsd:enumeration value="TP4"/>
										<xsd:enumeration value="TP5"/>
										<xsd:enumeration value="TP6"/>
										<xsd:enumeration value="TP7"/>
										<xsd:enumeration value="TR1"/>
										<xsd:enumeration value="TR2"/>
										<xsd:enumeration value="TR3"/>
										<xsd:enumeration value="TR4"/>
										<xsd:enumeration value="TU1"/>
										<xsd:enumeration value="TU2"/>
										<xsd:enumeration value="TU3"/>
										<xsd:enumeration value="TY1"/>
										<xsd:enumeration value="VE1"/>
										<xsd:enumeration value="VE2"/>
										<xsd:enumeration value="VG1"/>
										<xsd:enumeration value="VI1"/>
										<xsd:enumeration value="WA1"/>
										<xsd:enumeration value="WH1"/>
										<xsd:enumeration value="WH2"/>
									</xsd:simpleType>
								</xsd:element>
								<xsd:element name="comments" minOccurs="0">
									<xsd:simpleType base="xsd:string">
										<xsd:maxLength value="255"/>
									</xsd:simpleType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:attribute name="sheet" type="xsd:positiveInteger" use="required"/>
					<xsd:attribute name="holding" type="eforms:CPHnumType" use="required"/>
					<xsd:attribute name="yieldregion" type="eforms:YieldRegionType" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of eformsStructure type definition -->
	<!-- ==================== -->
	<!-- Common Complex Types -->
	<!-- ==================== -->
	<!-- start of FarmerStructure type definition -->
	<xsd:complexType name="FarmerStructure">
		<xsd:annotation>
			<xsd:documentation>The structure holds standard data about an individual farmer, including name, address and business details, together with any amendments that have been made.  This corresponds to questions 2 and 4 of the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="title" type="eforms:CitizenNameTitleType" minOccurs="0"/>
			<xsd:element name="initials">
				<xsd:simpleType base="xsd:string">
					<xsd:minLength value="0"/>
					<xsd:maxLength value="12"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="surname" type="eforms:CitizenNameSurnameType"/>
			<xsd:element name="businessname">
				<xsd:simpleType base="xsd:string">
					<xsd:minLength value="1"/>
					<xsd:maxLength value="70"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="address1" type="eforms:AddressLineType"/>
			<xsd:element name="address2" type="eforms:AddressLineType"/>
			<xsd:element name="address3" type="eforms:AddressLineType" minOccurs="0"/>
			<xsd:element name="town" type="eforms:AddressTownType"/>
			<xsd:element name="county" type="eforms:AddressLineType"/>
			<xsd:element name="postcode" type="eforms:AddressPostCodeUKtype"/>
			<xsd:element name="country" type="eforms:AddressTownType"/>
			<xsd:element name="mainholding" type="eforms:CPHnumType"/>
			<xsd:element name="telephone" type="eforms:TelephoneNumberType"/>
			<xsd:element name="vendorno" type="eforms:VendorNumberType"/>
			<xsd:element name="faxno" type="eforms:TelephoneNumberType" minOccurs="0"/>
			<xsd:element name="mobileno" type="eforms:TelephoneNumberType" minOccurs="0"/>
			<xsd:element name="emailaddr" type="eforms:EmailAddressType" minOccurs="0"/>
			<xsd:element name="amend" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Amendments that have been made to the farmers details are held in this structure, this corresponds to question 3 on the base form.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence minOccurs="0">
						<xsd:element name="title" type="eforms:CitizenNameTitleType"/>
						<xsd:element name="initials">
							<xsd:simpleType base="xsd:string">
								<xsd:minLength value="0"/>
								<xsd:maxLength value="12"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="surname" type="eforms:CitizenNameSurnameType"/>
						<xsd:element name="businessname">
							<xsd:simpleType base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="70"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="address1" type="eforms:AddressLineType"/>
						<xsd:element name="address2" type="eforms:AddressLineType"/>
						<xsd:element name="address3" type="eforms:AddressLineType"/>
						<xsd:element name="town" type="eforms:AddressTownType"/>
						<xsd:element name="county" type="eforms:AddressLineType"/>
						<xsd:element name="postcode" type="eforms:AddressPostCodeUKtype"/>
						<xsd:element name="country" type="eforms:AddressTownType"/>
						<xsd:element name="telephone" type="eforms:TelephoneNumberType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of FarmerStructure type definition -->
	<!-- Start of AreaAidNonFoodStructure type definition -->
	<xsd:complexType name="AreaAidNonFoodStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 5 from the base form, each tag holds either 1 or 0 for Yes or No respectively.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="firsttime" type="eforms:YesNoType"/>
			<xsd:element name="nfsetaside" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of AreaAidNonFoodStructure type definition -->
	<!-- Start of OtherHoldingsStructure type definition -->
	<xsd:complexType name="OtherHoldingsStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 6 from the base form, each line is represented by a holding node.</xsd:documentation>
		</xsd:annotation>
		<xsd:element name="holding" minOccurs="0" maxOccurs="unbounded">
			<xsd:complexType>
				<xsd:sequence>
					<xsd:element name="cph" type="eforms:CPHnumType"/>
					<xsd:element name="area" type="eforms:AreaType"/>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
	</xsd:complexType>
	<!-- End of OtherHoldingsStructure type definition -->
	<!-- Start of FormationDateStructure type definition -->
	<xsd:complexType name="FormationDateStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 7 from the base form, the reasonformed node is a free text entry field of 255 chars.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="formed920630" type="eforms:YesNoType"/>
			<xsd:element name="reasonformed" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of FormationDateStructure type definition -->
	<!-- Start of OtherBusinessesStructure type definition -->
	<xsd:complexType name="OtherBusinessesStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 8 from the base form, part a corresponds to otherland, each row of part b is represented by a business node, c is the deemedseparate node, nochanges holds part d, and changedetails is part e which can be up to 4Kb.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="otherland" type="eforms:YesNoType"/>
			<xsd:element name="deemedseparate" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nochanges" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="changedetails" type="eforms:ExtraLongStringType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="business" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="nameaddr" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="cph" type="eforms:CPHnumType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="involvement" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="reasonformed" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of OtherBusinessesStructure type definition -->
	<!-- Start of SheepProducerStructure type definition -->
	<xsd:complexType name="SheepProducerStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 9 from the base form, each box for the 2nd part of part c is represented by a member node.  Part d of the question is represented by nameaddr, postcode and phone nodes.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="producergroup" type="eforms:YesNoType"/>
			<xsd:element name="single" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="separateapps" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nameaddr" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="postcode" type="eforms:AddressPostCodeUKtype" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="phone" type="eforms:TelephoneNumberType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="member" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="details" type="eforms:LongStringType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of SheepProducerStructure type definition -->
	<!-- Start of AgriEnvironmentStructure type definition -->
	<xsd:complexType name="AgriEnvironmentStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 10 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="energycrops" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="envsens" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="stewardship" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="access" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="habitat" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nitrate" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="organic" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="orgfarm" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="moorland" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="woodland" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="woodgrant" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of AgriEnvironmentStructure type definition -->
	<!-- Q. 11 is a simple Yes/No type so is inline above -->
	<!-- Start of ECregimesStructure type definition -->
	<xsd:complexType name="ECregimesStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 12 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="dehydfodder" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="seedprod" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="hopsincome" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of ECregimesStructure type definition -->
	<!-- Start of ForageStructure type definition -->
	<xsd:complexType name="ForageStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of questions 13 and 14 from the base form, each row from part b is represented by a details node</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="forage" type="eforms:YesNoType"/>
			<xsd:element name="foragearea" type="eforms:AreaType" minOccurs="0"/>
			<xsd:element name="allland" type="eforms:YesNoType" minOccurs="0"/>
			<xsd:element name="foragedetails" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fdpsheet" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="fdpline" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="dates" type="eforms:LongStringType"/>
						<xsd:element name="prevdetails" type="eforms:LongStringType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of ForageStructure type definition -->
	<!-- Start of SharedGrazingStructure type definition -->
	<xsd:complexType name="SharedGrazingStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 15 from the base form, a details node holds information for each row.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence minOccurs="0">
			<xsd:element name="sharedgrazing" type="eforms:YesNoType"/>
			<xsd:element name="grazingdetails" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fdpsheet" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="fdpline" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="users" type="eforms:CPHnumType"/>
						<xsd:element name="sharing" type="eforms:PercentageType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of SharedGrazingStructure type definition -->
	<!-- Start of CommonLandStructure type definition -->
	<xsd:complexType name="CommonLandStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of questions 16, 17a) and 17b) from the base form, a registered node holds information for each row of question 17a) and a borrowed node for question 17b).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="commongrazing" type="eforms:YesNoType"/>
			<xsd:element name="registered" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="commonname" type="eforms:LongStringType"/>
						<xsd:element name="clnumber" type="eforms:CLnumberType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="CLdetails" type="eforms:CLdetailsStructure"/>
						<xsd:element name="lfasheepprod" type="eforms:YesNoType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="borrowed" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="commonname" type="eforms:LongStringType"/>
						<xsd:element name="clnumber" type="eforms:CLnumberType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="rightsname" type="eforms:LongStringType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="rightscph" type="eforms:CPHnumType" minOccurs="0" maxOccurs="1"/>
						<xsd:element name="CLdetails" type="eforms:CLdetailsStructure"/>
						<xsd:element name="lfasheepprod" type="eforms:YesNoType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of CommonLandStructure type definition -->
	<!-- Start of CLdetailsStructure type definition -->
	<xsd:complexType name="CLdetailsStructure">
		<xsd:annotation>
			<xsd:documentation>This node defines the structure for the common columns A, B and C from questions 17a) and 17b).</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="GrazingAllowance">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="number" type="xsd:positiveInteger" minOccurs="0"/>
						<xsd:element name="type" minOccurs="0">
							<xsd:simpleType base="xsd:string">
								<xsd:enumeration value="Cattle"/>
								<xsd:enumeration value="Sheep"/>
								<xsd:enumeration value="Pigs"/>
								<xsd:enumeration value="Goats"/>
								<xsd:enumeration value="Ponies"/>
								<xsd:enumeration value="Horses"/>
								<xsd:enumeration value="Donkeys"/>
								<xsd:enumeration value="Geese"/>
								<xsd:enumeration value="Other Poultry"/>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TotalLUs" type="eforms:RestrictedNumericType" minOccurs="0"/>
			<xsd:element name="Stints" type="eforms:RestrictedNumericType" minOccurs="0"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- End of CLdetailsStructure type definition -->
	<!-- Start of SetAsideLandStructure type definition -->
	<xsd:complexType name="SetAsideLandStructure">
		<xsd:annotation>
			<xsd:documentation>Questions 18 and 19 are simple Yes or No answers so appear in-line in eformsStructure.  This node holds details of question 20 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="setasidestrip10" type="eforms:YesNoType"/>
			<xsd:element name="setasidestrip20" type="eforms:YesNoType"/>
			<xsd:element name="setasidepart" type="eforms:YesNoType"/>
			<xsd:element name="setasidewhole" type="eforms:YesNoType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of SetAsideLandStructure type definition -->
	<!-- Start of FWPSStructure type definition -->
	<xsd:complexType name="FWPSsetasideStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 21 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="fwpssetaside" type="eforms:YesNoType"/>
			<xsd:element name="fwparea" type="xsd:decimal" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of FWPSStructure type definition -->
	<!-- Start of VoluntarySetasideStructure type definition -->
	<xsd:complexType name="VoluntarySetasideStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 22 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="voluntarysetaside" type="eforms:YesNoType"/>
			<xsd:element name="fiveyear" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="biomass" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reversion" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- End of VoluntarySetasideStructure type definition -->
	<!--	Start of GuaranteedSetasideStructure type definition -->
	<xsd:complexType name="GuaranteedSetasideStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 23 from the base form.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="setasidescheme" minOccurs="0" maxOccurs="1">
				<xsd:simpleType base="xsd:string">
					<xsd:minLength value="0"/>
					<xsd:maxLength value="22"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="caschemestart" minOccurs="0" maxOccurs="1">
				<xsd:simpleType base="xsd:string">
					<xsd:minLength value="0"/>
					<xsd:maxLength value="22"/>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--	End of GuaranteedSetasideStructure type definition -->
	<!--	Start of ArableSetasideStructure type definition -->
	<xsd:complexType name="ArableSetasideStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 24-26 from the base form, there is a details node for each row from the third part of this question.  YieldRegionType is a enumerated type giving details of all the available regions, it is defined in core.xsd</xsd:documentation>
		</xsd:annotation>
		<xsd:element name="multiannual" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		<xsd:element name="arablesetaside" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		<xsd:element name="border" type="eforms:YesNoType" minOccurs="0" maxOccurs="1"/>
		<xsd:element name="arabledetails" minOccurs="0" maxOccurs="1">
			<xsd:complexType>
				<xsd:sequence>
					<xsd:element name="arablearea" type="xsd:decimal"/>
					<xsd:element name="yieldregionarable" type="eforms:YieldRegionType"/>
					<xsd:element name="setasidearea" type="xsd:decimal"/>
					<xsd:element name="yieldregionsetaside" type="eforms:YieldRegionType"/>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
	</xsd:complexType>
	<!--	End of ArableSetasideStructure type definition -->
	<!--	Start of TransferredLandStructure type definition -->
	<xsd:complexType name="TransferredLandStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 27 from the base form, in contrast to the hardcopy version of the IACS2 base form, it is possible to enter more than one set of details, each set is held in a details node.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="aaps" type="eforms:YesNoType"/>
			<xsd:element name="transferreddetails" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fdpsheet" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="fdpline" type="eforms:RestrictedIntegerType"/>
						<xsd:element name="nameaddr" type="eforms:LongStringType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--	End of TransferredLandStructure type definition -->
	<!--	Start of YieldSummaryStructure type definition -->
	<xsd:complexType name="YieldSummaryStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of question 29 from the base form, each entry on the grid is represented by an area node.</xsd:documentation>
		</xsd:annotation>
		<xsd:element name="area" minOccurs="0" maxOccurs="unbounded">
			<xsd:complexType derivedBy="extension" base="eforms:AreaType">
				<xsd:attribute name="region" type="eforms:YieldRegionType" use="required"/>
				<xsd:attribute name="group" type="eforms:CropGroupType" use="required"/>
			</xsd:complexType>
		</xsd:element>
	</xsd:complexType>
	<!--	End of YieldSummaryStructure type definition -->
	<!--	Start of SupportingDocsStructure type definition -->
	<xsd:complexType name="SupportingDocsStructure">
		<xsd:annotation>
			<xsd:documentation>This node holds details of supporting documents question (Section IV) from the base form, each item can contain an integer between 0 and 255.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="sketchmap" type="eforms:RestrictedIntegerType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="iacs22" type="eforms:RestrictedIntegerType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="contractnonfood" type="eforms:RestrictedIntegerType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="glucosinolate" type="eforms:RestrictedIntegerType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="contracterucic" type="eforms:RestrictedIntegerType" minOccurs="0" maxOccurs="1"/>
