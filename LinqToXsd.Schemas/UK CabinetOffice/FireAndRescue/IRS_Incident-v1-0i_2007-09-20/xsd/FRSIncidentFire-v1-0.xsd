	<xs:complexType name="FireStructure">
		<xs:annotation>
			<xs:documentation>Details all the information that will be recorded for any type of fire incident.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsPrimaryFire" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>Was this a Primary fire? (Not a Secondary Fire). This flag is determined from a number of other attributes 3.5 to 3.9.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cause" type="FireCauseType">
				<xs:annotation>
					<xs:documentation>5.15 - The main cause/motive of this fire, ie accidental or deliberate.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsDerelict" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>3.8 - Was the Property Type derelict (Building, Boat, Road Vehicle, etc.)?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Building" type="BuildingStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Extra information only needed for building fires.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OutdoorArea" type="OutdoorAreaStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Extra information only needed for outdoor fires.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrimaryFire" type="PrimaryFireStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Extra information only needed for Primary Fires.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BuildingStructure">
		<xs:annotation>
			<xs:documentation>Information that should be recorded for all types of building fire</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NormalOccupationStatus" type="BuildingOccupationStatusType">
				<xs:annotation>
					<xs:documentation>5.14 - Is the building normally occupied or  un-oocupied most of the time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OccupiedAtTimeOfIncident" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>5.13 - Was the building occupied at the time of the fire incident?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsChimneyFire" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>3.9 - Was the fire contained solely within the Chimney of a residential Building?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OutdoorAreaStructure">
		<xs:annotation>
			<xs:documentation>Contains information recorded for all outdoor fires.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DamageArea">
				<xs:annotation>
					<xs:documentation>5.16 (Secondary Fire) / 8.35 (Primary Fire) - The size of the damaged area held in square metres up to 10,000 m2.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AreaRangeType">
							<xs:attribute name="LargeAreaSize" type="fire:ZeroTo9999999Type">
								<xs:annotation>
									<xs:documentation>5.16a / 8.35a - The area in hectares if the outdoor area is greater than 10,000 m2 (1 hectare) captured in the DamageArea.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="IsNationalPark" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>5.17 (Secondary Fire) / 8.36 (Primary Fire) - Was the area inside a National Park?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PrimaryFireStructure">
		<xs:annotation>
			<xs:documentation>Contains all the extra information that is recorded only for PrimaryFires</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsMultiSeated" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>5.1 - Did the fire have multiple seats (start locations)?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsHeatSmokeDamageOnly" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>8.19 - Was there no exposed flame, but heat or smoke damage only?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Discovery" type="PFDiscoveryStructure"/>
			<xs:element name="CauseAndReason" type="PFCauseAndReasonStructure">
				<xs:annotation>
					<xs:documentation>Information on the cause of the fire and the reasons for its spread.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Action" type="PFActionStructure">
				<xs:annotation>
					<xs:documentation>Details the actions taken and facilities used to combat the fire</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>This choice block is determined by the incident type data, whcih details the type of location that the fire is in.</xs:documentation>
				</xs:annotation>
				<xs:element name="Dwelling" type="DwellingStructure">
					<xs:annotation>
						<xs:documentation>House, Bungalow, Mobile Home, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OtherResidential" type="OtherResidentialStructure">
					<xs:annotation>
						<xs:documentation>e.g. Nursing Home, Motel, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NonResidential" type="NonResidentialStructure">
					<xs:annotation>
						<xs:documentation>e.g. Offices, Shop, Warehouse, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RoadVehicle" type="RoadVehicleStructure">
					<xs:annotation>
						<xs:documentation>e.g. Passenger Car, Van, Lorry, Tanker, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Aircraft" type="AircraftStructure">
					<xs:annotation>
						<xs:documentation>e.g. Passenger plane, Helicopter, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Boat" type="BoatStructure">
					<xs:annotation>
						<xs:documentation>e.g. Barge, Motor Yacht, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RailVehicle" type="RailVehicleStructure">
					<xs:annotation>
						<xs:documentation>e.g. Passenger Train, Freight Train, Steam Train, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Outdoor" type="OutdoorStructure">
					<xs:annotation>
						<xs:documentation>e.g. Forest, Woodland, RiverBank, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OutdoorStructure" type="OutdoorStructureStructure">
					<xs:annotation>
						<xs:documentation>e.g. Refuse Tip, Post Box, Road Furniture, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BuildingOccupationStatusType">
		<xs:annotation>
			<xs:documentation>Defines the ocupation staus of the building normally.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Yes - occupied</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>No - unoccupied permanently (vacant)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>No - under construction</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<!--			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Unoccupied</xs:documentation>
				</xs:annotation>
			</xs:enumeration> -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FireCauseType">
		<xs:annotation>
			<xs:documentation>Lists the available options for indicating the cause/motive of the fire</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Accidental</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Deliberate - own property</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Deliberate - others property</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Deliberate - unknown owner</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="PFCauseAndReasonStructure">
		<xs:annotation>
			<xs:documentation>Structure containing detailed information on the cause and reason for spread of a primary fire</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MainCause">
				<xs:annotation>
					<xs:documentation>8.1 - Main cause e.g. playing with fire</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="PFMainCauseStructure">
							<xs:attribute name="MakeModelDescription" type="fire:StringRestrictedMixedType">
								<xs:annotation>
									<xs:documentation>8.2 - Dependant on MainCause being set to '4 = Faulty lead to equipment or appliance', '5 = fault in equipment or appliance', 15 = Vehicle crash or collision or ’99 = Other’</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="IgnitionSource" type="PFIgnitionSourceStructure">
				<xs:annotation>
					<xs:documentation>8.4 - Source of ignition e.g. heating equipment. The annotations define an applicable IgnitionSourcePower setting, or that the user must select a value. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IgnitionSourcePower" type="PFIgnitionSourcePowerStructure">
				<xs:annotation>
					<xs:documentation>8.5 - The power resource that enabled ignition e.g. gas, electricity. This value must be populated if Ignition Source is a powered source.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CausedBy" type="PFCausedByStructure">
				<xs:annotation>
					<xs:documentation>8.3 - Caused by person, appliance, etc
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemFirstIgnited" type="PFItemIgnitedStructure">
				<xs:annotation>
					<xs:documentation>8.6 - Item first ignited e.g. furniture.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemResponsibleForSpread" type="PFItemIgnitedStructure">
				<xs:annotation>
					<xs:documentation>8.7 - Item responsible for spread e.g. foam.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RapidGrowth" type="PFRapidGrowthStructure">
				<xs:annotation>
					<xs:documentation>8.8 - Rapid fire growth due to e.g. backdraft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DangerousSubstances" type="PFDangerousSubstanceType">
				<xs:annotation>
					<xs:documentation>8.9 - Were dangerous substance involved e.g. Ammunitions, Fireworks.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExplosionInvolved" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>8.10 - Was an Explosion Involved?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExplosionDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Explosion details if there was an explosion.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ExplosionSubstances" type="PFExplosionSubstanceType">
							<xs:annotation>
								<xs:documentation>8.11 - Explosion due to substance involved e.g. Ammunitions, Fireworks.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExplosionStage" type="PFExplosionStageType">
							<xs:annotation>
								<xs:documentation>8.12 - Was it before fire, during fire, not fire related, etc.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExplosionContainers" type="PFExplosionContainersStructure">
							<xs:annotation>
								<xs:documentation>8.13 - What was the container that exploded e.g. Cylinder, Drum, etc. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PFMainCauseStructure">
		<xs:annotation>
			<xs:documentation>Extends PFMainCauseType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="PFMainCauseType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="PFMainCauseType">
		<xs:annotation>
			<xs:documentation>Details the available options for specifying the main cause of a primary fire</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Faulty fuel supply - gas</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Faulty fuel supply - electricity</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Faulty fuel supply - petrol product</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Faulty leads to equipment or appliance</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Fault in equipment or appliance</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Cooking - chip pan/deep fat fryer</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Cooking - other cooking</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation>Negligent use of equipment or appliance (heat source)</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Playing with fire (or heat source)</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation>Careless handling - due to sleep or unconsciousness</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation>Careless handling  - due to careless disposal</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="14">
				<xs:annotation>
					<xs:documentation>Careless handling - due to knocking over</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="15">
				<xs:annotation>
					<xs:documentation>Combustible articles too close to heat source (or fire)</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="16">
				<xs:annotation>
					<xs:documentation>Person too close to heat source (or fire)</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="17">
				<xs:annotation>
					<xs:documentation>Vehicle crash or collision</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="18">
				<xs:annotation>
					<xs:documentation>Chimney fire</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="19">
				<xs:annotation>
					<xs:documentation>Bonfire going out of control</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="20">
				<xs:annotation>
					<xs:documentation>Other intentional burning, going out of control</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="21">
				<xs:annotation>
					<xs:documentation>Accumulation of flammable material</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="22">
				<xs:annotation>
					<xs:documentation>Natural occurrence</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="23">
				<xs:annotation>
					<xs:documentation>Overheating, unknown cause</xs:documentation>
					<xs:appinfo>
						<FireCause>1</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="25">
				<xs:annotation>
					<xs:documentation>Bomb/incendiary device</xs:documentation>
					<xs:appinfo>
						<FireCause>2</FireCause>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="26">
				<xs:annotation>
