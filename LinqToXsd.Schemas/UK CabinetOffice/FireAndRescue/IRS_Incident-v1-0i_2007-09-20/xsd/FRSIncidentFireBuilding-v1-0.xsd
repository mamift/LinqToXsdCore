	<xs:complexType name="DwellingStructure">
		<xs:annotation>
			<xs:documentation>Contains all the specific details needed for a dwelling primary fire.  This extends the PFBuildingStructure</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PFBuildingStructure">
				<xs:sequence>
					<xs:element name="FireStartLocation" type="DwellingFireStartLocationStructure">
						<xs:annotation>
							<xs:documentation>8.14 - Location of fire start e.g. kitchen, engine room.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CauseAndReason" type="DwellingCauseAndReasonStructure">
						<xs:annotation>
							<xs:documentation>Contains the additional cause and reason details that only apply to dwelling fires.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherResidentialStructure">
		<xs:annotation>
			<xs:documentation>Contains all the specific details needed for an 'OtherResidential' primary fire.  This extends the PFBuildingStructure</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PFBuildingStructure">
				<xs:sequence>
					<xs:element name="FireStartLocation" type="OtherResedentialFireStartLocationStructure">
						<xs:annotation>
							<xs:documentation>8.14 - Location of fire start e.g. kitchen.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CauseAndReason" type="OtherCauseAndReasonStructure" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains the additional cause and reason details that apply to other residential fires</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NonResidentialStructure">
		<xs:annotation>
			<xs:documentation>Contains all the specific details needed for a 'NonResidential' primary fire.  This extends the PFBuildingStructure</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PFBuildingStructure">
				<xs:sequence>
					<xs:element name="FireStartLocation" type="NonResedentialFireStartLocationStructure">
						<xs:annotation>
							<xs:documentation>8.14 - Location of fire start e.g. kitchen.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CauseAndReason" type="OtherCauseAndReasonStructure" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains the additional cause and reason details that apply to non resedential fires</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PFBuildingStructure">
		<xs:annotation>
			<xs:documentation>Contains all the elements that are required by all types of buildings.  This should be extended by the above three more specific building types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SpecialConstruction" type="SpecialBuildingConstructionStructure">
				<xs:annotation>
					<xs:documentation>8.18 - Special Building construction involved e.g. sandwich panels.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EvacuationTime" type="EvacuationTimePeriodType">
				<xs:annotation>
					<xs:documentation>9.5 - Estimated time for evacuation to be completed e.g. 5 to 30 minutes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EvacuationDelay" type="EvacuationDelayType">
				<xs:annotation>
					<xs:documentation>9.4 - Was there a delay in completing building evacuation e.g. Means of escape â€“ exits locked.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SafetySystems" type="BuildingSafetySystemsStructure"/>
			<xs:element name="Damage" type="BuildingDamageStructure"/>
			<xs:element name="FireLocation" type="BuildingFireLocationStructure"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SpecialBuildingConstructionStructure">
		<xs:annotation>
			<xs:documentation>Extends SpecialBuildingConstructionType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="SpecialBuildingConstructionType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="SpecialBuildingConstructionType">
		<xs:annotation>
			<xs:documentation>Defines the options available for indicating whether or not any special construction methods have been used on the building.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation>None</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Timber framed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Cladding </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Sandwich panels</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Atria</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Thatch </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Large single storey retail premises</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EvacuationTimePeriodType">
		<xs:annotation>
			<xs:documentation>A generic type for indicating the time period that has passed. Includes a "Not applicable" option.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Immediately</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Under 5 minutes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>5 to 30 minutes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Over 30 minutes and up to 2 hours</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Over 2 hours</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="98">
				<xs:annotation>
					<xs:documentation>Not applicable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EvacuationDelayType">
		<xs:annotation>
			<xs:documentation>The options available for specifing whether there was a delay in evacuation, and if so what caused it.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Evacuation, but no delay</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Delay due to: Firefighting actions - public</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Delay due to: Occupants did not respond to automatic alarm</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Delay due to: Elderly or disabled</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Delay due to: Re-entered building</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Delay due to: Building management poor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation>Delay due to: Building layout/signage poor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Delay due to: Firefighting actions by fire service - contraflow on stairs</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation>Delay due to: Means of escape - exits locked</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation>Delay due to: Means of escape - items stored</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="12">
				<xs:annotation>
					<xs:documentation>Delay due to: Means of escape - not suitable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="13">
				<xs:annotation>
					<xs:documentation>Delay due to: Means of escape - other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="14">
				<xs:annotation>
					<xs:documentation>Delay due to: Occupants gathering possessions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="98">
				<xs:annotation>
					<xs:documentation>Not applicable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BuildingSafetySystemsStructure">
		<xs:annotation>
			<xs:documentation>Details any safety systems present in the building involved in the fire, and the affect they had.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MeansOfEscape" type="MeansOfEscapeTypeStructure">
				<xs:annotation>
					<xs:documentation>5.6 - Details of the effect of safety regarding means of escape.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Compartmentation" type="CompartmentationTypeStructure">
				<xs:annotation>
					<xs:documentation>5.7 - The effect of safety compartmentation had on the Fire.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AlarmSystemsPresent">
				<xs:annotation>
					<xs:documentation>Contains information on all alarm systems present in the building</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AlarmSystemsPresentStructure"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="SafetySystemsPresent">
				<xs:annotation>
					<xs:documentation>Contains information on all the active firefighting safety systems that were present in the building</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="SafetySystemsPresentStructure"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SafetySystemsPresentStructure">
		<xs:annotation>
			<xs:documentation>This structure contains information on all safety systems present in the building</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="System" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This structure allows for any number of safety systems to be defined, but in reality, the forms may only make provision for entered e.g. 3 of them.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Type" type="AFSafetySystemsTypeStructure">
							<xs:annotation>
								<xs:documentation>7.12 - Active System present in vicinity of fire e.g. sprinklers.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Location" type="SafetySystemsLocationType">
							<xs:annotation>
								<xs:documentation>7.13 - Location of system e.g. room of origin.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Operated" type="SafetySystemsOperatedType">
							<xs:annotation>
								<xs:documentation>7.14 - Did system operate e.g. Yes and raised alarm.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NumberOperated" type="AFSafetySystemsNumberOperatedType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>7.15 - Number od system type operated e.g. number of sprinklers. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Impact" type="AFSafetySystemsImpactType">
							<xs:annotation>
								<xs:documentation>7.16 - Impact made by safety system. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReasonForPoorOutcome" type="AFSafetySystemsPoorOutcomeStructure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>7.17 - Poor outcome e.g. Alarm battery missing.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="WereSafetySystemsPresent" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>7.11 - Were Fire Safety Systems present?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MeansOfEscapeTypeStructure">
		<xs:annotation>
			<xs:documentation>Extends MeansOfEscapeType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="MeansOfEscapeType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="MeansOfEscapeType">
		<xs:annotation>
			<xs:documentation>Options for defining the means of escape and potential problem.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>OK - no visible concerns</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Exits locked</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Exits blocked (e.g. Materials stored blocking exit)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Exit route blocked by smoke/flames</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Poor implementation e.g. doors swing the wrong way</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Contents contributing to abnormal fire spread /smoke production</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="98">
				<xs:annotation>
					<xs:documentation>Not applicable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CompartmentationTypeStructure">
		<xs:annotation>
			<xs:documentation>Extends CompartmentationType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="CompartmentationType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="CompartmentationType">
		<xs:annotation>
			<xs:documentation>Options for defining the compertmentation systems present.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Stopped/Checked spread</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Breached - current building work</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Breached - previous building work</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Breached - fire doors left open or incorrectly fitted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Damage to compartmentation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Fire spread through gaps or voids in construction</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="90">
				<xs:annotation>
					<xs:documentation>No compartmentation in building</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="98">
				<xs:annotation>
					<xs:documentation>Not applicable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AlarmSystemsPresentStructure">
		<xs:sequence>
			<xs:element name="System" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This structure allows for any number of alarm systems to be defined, but in reality, the forms may only make provision for entered e.g. 3 of them.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Type" type="AlarmSystemsTypeStructure">
							<xs:annotation>
								<xs:documentation>5.9 - Type of Alarm system.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Location" type="SafetySystemsLocationType">
							<xs:annotation>
								<xs:documentation>5.10 - Location of Alarm system.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Operated" type="SafetySystemsOperatedType">
							<xs:annotation>
								<xs:documentation>5.11 - Did the Alarm system operate?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReasonForPoorOutcome" type="AlarmSystemsPoorOutcomeStructure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>5.12 - If the Alarm system did not operate correctly, why?</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="WereAlarmSystemsPresent" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>5.8 - Were Fire Alarm Systems present?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AlarmSystemsTypeStructure">
		<xs:annotation>
			<xs:documentation>Extends AlarmSystemsPresentType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="AlarmSystemsPresentType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="AlarmSystemsPresentType">
		<xs:annotation>
			<xs:documentation>Represents the type of alarm system.  This provides an enumeration list of the available options.</xs:documentation>
		</xs:annotation>
