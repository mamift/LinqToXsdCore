	<xs:complexType name="IncidentStructure">
		<xs:annotation>
			<xs:documentation>The top level structure containing all information about an FRS Incident</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Identifier">
				<xs:annotation>
					<xs:documentation>The information that uniquely defines an incident</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="IdentifierStructure">
							<xs:attributeGroup ref="IncidentIdentifierAttributes"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ParentIncident" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identification details of the parent incident of this incident if there is one.This enables the sharing of Resource details across multiple incidents.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="IdentifierStructure">
							<xs:attributeGroup ref="IncidentIdentifierAttributes"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AuditDetail" type="AuditDetailStructure">
				<xs:annotation>
					<xs:documentation>Contains information useful for auditing this incident, such as its current state, along with info on when and by whom it was created and last updated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleParty" type="ResponsiblePartyStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The party responsible for completing and publishing the Incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentLocation" type="fire:LocationStructure">
				<xs:annotation>
					<xs:documentation>4.1 to 4.3b - The location at which the incident took place.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentAtCall" type="IncidentAtCallStructure">
				<xs:annotation>
					<xs:documentation>Stores the type of this incident as understood from the original call</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentOnAttendance" type="IncidentOnAttendanceStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Stores the information on this incident as known after attendance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResourceUsage" type="ResourceStructure">
				<xs:annotation>
					<xs:documentation>Information on all resources used in dealing with this incident.  This includes vehicles (appliances), equipment, and fire officers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalContent" type="fire:ContextDataStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Element that allows any XML content so that other relevant details can be associated with the incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>10.4, 10.5, 10.6 - Element to store any additional (String) information related to the incident. eg comments</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="InfoType" type="fire:StringRelaxedMixedType" use="required">
								<xs:annotation>
									<xs:documentation>The following fixed values have been allocated: ‘Summary’, ‘Recorded with Queries’, ‘Published with Conditions’, ‘Queried by National Statistics’. Other values can be used.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IncidentTypeLevelType">
		<xs:annotation>
			<xs:documentation>This needs to be restricted properly once confirmed.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="fire:StringRestrictedMixedType"/>
	</xs:simpleType>
	<xs:complexType name="IncidentAtCallStructure">
		<xs:annotation>
			<xs:documentation>Provides the information known about the fire after the initial call</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MobiliseIncidentType">
				<xs:annotation>
					<xs:documentation>2.3 - Describes the type of this incident as initially understood, by using one of the FireControl Values.  This actually stores the ID value from the appropriate taxonomy.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="fire:TaxonomyIDType">
							<xs:attribute name="GenericType" type="fire:GenericIncidentType" use="required">
								<xs:annotation>
									<xs:documentation>The GenericType defines a broad categorisation of the Mobilisation Incident Types. The value is set to a attribute value from the Taxonomy file.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Description" type="fire:StringRestrictedMixedType" use="optional">
								<xs:annotation>
									<xs:documentation>The Description needs to be populated if the Mobilisation IncidentType is set to 'Not known' as the command and control system supports text types only.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Other" type="fire:StringRestrictedMixedType" use="optional">
								<xs:annotation>
									<xs:documentation>Defines the attributes that should be added to any enumeration list where there is an 'other' option available.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PriorityAtCall" type="fire:PriorityType">
				<xs:annotation>
					<xs:documentation>2.3a - The initial priority that was assigned to this incident. Defaulted from the mobilse incident type taxonomy file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateCall" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>2.4 - Identifies whether or not this was a late call. i.e. A fire occurred, but had already been put out.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TimeOfCall" type="fire:DateTime">
				<xs:annotation>
					<xs:documentation>2.1 - The data and time of the call</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginOfCall">
				<xs:annotation>
					<xs:documentation>2.2 - Identifies where the call came from, ie person or automated system.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="OriginOfCallStructure">
							<xs:attribute name="Description" use="optional">
								<xs:annotation>
									<xs:documentation>The Description needs to be populated if the OriginOfCall is set to 'Not known' as the command and control system supports text types only.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="TimeStoppedMobilising" type="fire:DateTime">
				<xs:annotation>
					<xs:documentation>2.5 - The date and time of the last vehicle was mobilised to the incident. The Stop Message Sent.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TimeIncidentClosed" type="fire:DateTime">
				<xs:annotation>
					<xs:documentation>2.6 - The date and time of the last vehicle was remoced from the scene of the incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OverTheBorder">
				<xs:annotation>
					<xs:documentation>1.5 - Was the incident over the border? Indicates that mobilisation request is to a location outside the FRS's territory.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="core:YesNoType">
							<xs:attribute name="OverTheBorderFRSIncidentIdentifier" type="FRSIncidentIdentifierType" use="optional">
								<xs:annotation>
									<xs:documentation>1.7 - The FRSIncidentIdentifier for the FRS who's terrtory the Incident occured within.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IncidentOnAttendanceStructure">
		<xs:annotation>
			<xs:documentation>Contains all the information known about the fire after attendance</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IncidentCategory" type="IncidentCategoryType">
				<xs:annotation>
					<xs:documentation>3.1 - Contains the final top level classification of this incident. (fire, special service, or false alarm)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriorityOnAttendance" type="fire:PriorityType">
				<xs:annotation>
					<xs:documentation>2.3a - The final priority value (1-5) assigned to this incident. Defaulted from Mobilise Incident Type Taxonomy.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PropertyType">
				<xs:annotation>
					<xs:documentation>3.2 - Stores the type of property involved in this incident. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="fire:TaxonomyIDType">
							<xs:attribute name="PropertyCategory" type="PropertyCategoryType" use="required">
								<xs:annotation>
									<xs:documentation>Defines the high-level Property Category used to decide the additional information required for the incident. Populated from the Property Type Taxonomy.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Other" type="fire:StringRestrictedMixedType" use="optional">
								<xs:annotation>
									<xs:documentation>Defines the attributes that should be added to any enumeration list where there is an 'other' option available.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="IsPropertyRegulated" type="core:YesNoType" use="required">
								<xs:annotation>
									<xs:documentation>5.5 - Defines whether the property is covered by the Regulatory Reform Order (RRO). This value is defaulted from the Property Type Taxonomy, but can be overriden. </xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generally there will be one of these three sections populated based on the IncidentCategory. However, for Fires the SpecialService structure could be populated if the Incident is a Fire that involves and RTC or HazardousMaterials.</xs:documentation>
				</xs:annotation>
				<xs:element name="Fire" type="FireStructure" minOccurs="0"/>
				<xs:element name="SpecialService" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="SpecialServiceStructure"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="FalseAlarm" type="FalseAlarmStructure" minOccurs="0"/>
			</xs:sequence>
			<xs:element name="InvolvementOfPersons" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details of people involved during the incident - Evacuations, Rescues, Injuries and Fatalities.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="EvacuationAssistanceInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.6 - Were people evacuated in the incident? </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EvacuationAssistance" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Provides information on the numbers of people evacuated from the incident, with or without assistance.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="EvacuationAssistanceStructure"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="VictimsInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.5 - Were people rescued, extricated, injured or killed in the incident?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Victims" type="VictimsStructure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Provides details on all the victims involved in the incident.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AttackOnFirefightersInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.10 - Was there attacks on firefighters at the incident?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AttackOnFirefighters" type="AttackOnFirefightersStructure" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This section is used to provide extra information for incidents of special interest</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchIncidentCriteriaStructure">
		<xs:annotation>
			<xs:documentation>The structure containing all the primary search criteria for Incidents.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IncidentDateRange">
				<xs:annotation>
					<xs:documentation>The date range for the incidents to be selected.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="StartDateTime" type="fire:DateTime">
							<xs:annotation>
								<xs:documentation>The start date for selecting incidents. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EndDateTime" type="fire:DateTime">
							<xs:annotation>
								<xs:documentation>The date range for the incidents to be selected.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DateRangeType" type="DateRangeTypeType">
							<xs:annotation>
								<xs:documentation>The date range will be processed against the Incidents for the TimeOfCall or LastUpdated Date/Time.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="ByFRS">
					<xs:annotation>
						<xs:documentation>Enables all incidents to be selected for: a) the nominated FRS Territory b) recorded by the FRS in any other territory c) recorded by any other FRS in the nominated FRS territory. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="FRSId" type="fire:FRSIdType">
								<xs:annotation>
									<xs:documentation>The identifier for the brigade FRS.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="StationId" type="fire:StationIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The Station identifier if supplied.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ByTerritoryCategory">
					<xs:annotation>
						<xs:documentation>Enables incidents to be selected based on combinations of: a) FRS Territory (National Statistics FRS Id) b) Responible Party that Recording an incident c) if the incident was over the border or not. At least a) or b) must be used.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Identifier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The information that uniquely defines an incident. Used to select a specific incident or a collection based on the National Statistics FRS ID (Territory FRS Id).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="SearchIdentifierStructure">
											<xs:attributeGroup ref="IncidentIdentifierAttributes"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="ResponsibleParty" type="ResponsiblePartyStructure" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The party responsible for completing, recording and publishing the Incident.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OverTheBorder" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Indicates that mobilisation request is to a location outside the FRS border selected from the Identifier or the ResponsibleParty FRS Id values.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="core:YesNoType">
											<xs:attribute name="OverTheBorderFRSIncidentIdentifier" type="FRSIncidentIdentifierType" use="optional">
												<xs:annotation>
													<xs:documentation>The FRSIncidentIdentifier used by territory FRS, if the incident is recorded by another FRS (alternate ResponsibleParty).   </xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="IncidentStatusRange">
				<xs:annotation>
					<xs:documentation>This range allows the user to select a specific range of Incident Status values to be returned. By default all Incidents are returned in the range 0 to 99. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IncidentStatusFrom" type="IncidentStatusType" default="0">
							<xs:annotation>
								<xs:documentation>Stores the current status of this incident e.g. 50=Closed (Mobilised Data Captured), 60=Recorded, 65=Recorded With Queries, 70=Published, 0=Cancelled</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IncidentStatusTo" type="IncidentStatusType" default="90">
							<xs:annotation>
								<xs:documentation>Stores the current status of this incident e.g. 50=Closed (Mobilised Data Captured), 60=Recorded, 65=Recorded With Queries, 70=Published, 0=Cancelled.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchControlRequestStructure">
		<xs:annotation>
			<xs:documentation>The structure containing control information if paging is used.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PageSize" type="xs:nonNegativeInteger" default="50">
				<xs:annotation>
					<xs:documentation>Number of match results per page.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PageNumber" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The page to be returned for the selection criteria.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderSearchBy" type="OrderSearchByType" default="TimeOfCall" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This allows a field to be used to define the sequence of the Search Results retuned e.g. TimeOfCall, FRSIncidentNumber. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderingType" default="Ascending" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This allows a field to be used to define the sorting sequence of a particular field - Ascending / Descending.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="OrderingTypeType">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchControlResultsStructure">
		<xs:annotation>
			<xs:documentation>The structure containing control information if paging is used.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PageSize" type="xs:nonNegativeInteger">
				<xs:annotation>
					<xs:documentation>Number of match results per page.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PageNumber" type="xs:nonNegativeInteger">
				<xs:annotation>
					<xs:documentation>The current page number. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalSize" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total number of search results returned for the search criteria.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalPageNumber" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total number of pages of matched results.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderSearchBy" type="OrderSearchByType" default="TimeOfCall" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This allows a field to be used to define the sequence of the Search Results retuned e.g. TimeOfCall, FRSIncidentNumber. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderingType" default="Ascending" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This allows a field to be used to define the sorting sequence of a particular field - Ascending / Descending.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="OrderingTypeType">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchIncidentFiltersStructure">
		<xs:annotation>
			<xs:documentation>The structure containing all elements that can be used to filter / restict the incidents returned for the Search Criteria.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostCode" type="bs7666:PostCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PostCode of the Incident Location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentCategory" type="IncidentCategoryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the final top level classification of this incident. (fire, special service, or false alarm)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BasicIncidentDetailsStructure">
		<xs:annotation>
			<xs:documentation>The structure containing the basic information about an Incident.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Identifier">
				<xs:annotation>
					<xs:documentation>The information that uniquely defines an incident.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="IdentifierStructure">
							<xs:attributeGroup ref="IncidentIdentifierAttributes"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ParentIncident" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identification details of the parent incident of this incident if there is one.This enables the sharing of Resource details across multiple incidents.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="IdentifierStructure">
							<xs:attributeGroup ref="IncidentIdentifierAttributes"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AuditDetail" type="BasicAuditDetailStructure">
				<xs:annotation>
					<xs:documentation>The basic audit details defining details such as: Incident Status, who created and last updated the Incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleParty" type="ResponsiblePartyStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The party responsible for completing and publishing the Incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TimeOfCall" type="fire:DateTime">
				<xs:annotation>
					<xs:documentation>The data and time of the call</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentLocation" type="fire:LocationStructure">
				<xs:annotation>
					<xs:documentation>The location at which the incident took place</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentCategory" type="IncidentCategoryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the final top level classification of this incident. (fire, special service, or false alarm)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OverTheBorder" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that mobilisation request is to a location outside the FRS borders.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="core:YesNoType">
							<xs:attribute name="OverTheBorderFRSIncidentIdentifier" type="FRSIncidentIdentifierType" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PropertyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>3.2 - Stores the type of property involved in this incident. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="fire:TaxonomyIDType">
							<xs:attribute name="PropertyCategory" type="PropertyCategoryType" use="required">
								<xs:annotation>
									<xs:documentation>Defines the high-level Property Category used to decide the additional information required for the incident. Populated from the Property Type Taxonomy.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Other" type="fire:StringRestrictedMixedType" use="optional">
								<xs:annotation>
									<xs:documentation>Defines the attributes that should be added to any enumeration list where there is an 'other' option available.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="IsPropertyRegulated" type="core:YesNoType" use="required">
								<xs:annotation>
									<xs:documentation>5.5 - Defines whether the property is covered by the Regulatory Reform Order (RRO). This value is defaulted from the Property Type Taxonomy, but can be overriden. </xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Fire" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IsPrimaryFire" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>Was this a Primary fire? (Not a Secondary Fire). This flag is determined from a number of other attributes 3.5 to 3.9.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsChimneyFire" type="core:YesNoType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>3.9 - Was the fire contained solely within the Chimney of a residential Building?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Cause" type="FireCauseType">
							<xs:annotation>
								<xs:documentation>5.15 - The main cause/motive of this fire, ie accidental or deliberate.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SpecialService" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SpecialServiceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>3.3 - Stores the type information for this special service incident.  This is the ID value from the associated Special Service taxonomy file.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="fire:TaxonomyIDType">
										<xs:attribute name="Other" type="fire:StringRestrictedMixedType" use="optional">
											<xs:annotation>
												<xs:documentation>Defines the attributes that should be added to any enumeration list where there is an 'other' option available.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="HazardousMaterialsInvolved" type="fire:YesNoDontKnowType">
							<xs:annotation>
								<xs:documentation>5.18 - Did the Incident involve Hazardous Materials?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsRTC" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>5.22 - Was the Incident an RTC?</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FalseAlarm" type="FalseAlarmStructure" minOccurs="0"/>
			<xs:element name="InvolvementOfPersons" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="VictimsInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.5 - Were people rescued, extricated, injured or killed in the incident?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EvacuationAssistanceInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.6 - Were people evacuated in the incident? </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AttackOnFirefightersInvolved" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>3.10 - Was there attacks on firefighters at the incident?</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ResourceUsage" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="NumberOfVehiclesDeployed" type="fire:ZeroTo999Type">
							<xs:annotation>
								<xs:documentation>3.7 - If more than 5 vehicles (appliances) deployed to a Fire incident then it will be categorised as a Primary Fire.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EquipmentUsed" type="core:YesNoType">
							<xs:annotation>
								<xs:documentation>6.15 - Were Equipment/Resources used at the Incident? Should be 'no' for False Alarms and Late Call Fires, but 'yes' for all other Fires and SpecialServices. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ValidationStatus" type="IncidentValidationStatusType" use="required"/>
	</xs:complexType>
	<xs:simpleType name="DateRangeTypeType">
		<xs:annotation>
			<xs:documentation>Defines the appropriate date time element to be used for seaching for Incidents.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
