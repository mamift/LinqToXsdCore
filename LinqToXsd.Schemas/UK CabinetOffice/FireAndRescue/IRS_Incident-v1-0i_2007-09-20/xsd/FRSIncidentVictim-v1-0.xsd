	<xs:complexType name="VictimSummaryStructure">
		<xs:annotation>
			<xs:documentation>Contains summary victim information, including fatalities, casualties and uninjured totals, as well as firefighter victims.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FatalVictims" type="VictimSummaryTotalsStructure">
				<xs:annotation>
					<xs:documentation>Summary of the people fataly injured at the scene of the Special Service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CasualtyVictims" type="VictimSummaryTotalsStructure">
				<xs:annotation>
					<xs:documentation>Summary of the people injured at the scene of the Special Service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnInjuredVictims" type="VictimSummaryTotalsStructure">
				<xs:annotation>
					<xs:documentation>Summary of the people removed, but with out injury at the scene of the Special Service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FirefighterCasualties">
				<xs:annotation>
					<xs:documentation>Summary of the Fire fighter casualties on attending the Special Service.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Fatalities" type="fire:ZeroTo9999999Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The number of firefighter fatalities.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SeriousInjuries" type="fire:ZeroTo9999999Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The number of firefighters with serious injuries, generally requiring hospitalisation.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SlightInjuries" type="fire:ZeroTo9999999Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The number of firefighters with minor injuries.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VictimSummaryTotalsStructure">
		<xs:annotation>
			<xs:documentation>A simple structure for specifying two totals. The number of people extricated (using special means), and the number released. This is applied to fatality, casualty, and uninjured victims.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NumberExtricated" type="fire:ZeroTo9999999Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number extricated using specialised equipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberReleased" type="fire:ZeroTo9999999Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number released by other means.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
