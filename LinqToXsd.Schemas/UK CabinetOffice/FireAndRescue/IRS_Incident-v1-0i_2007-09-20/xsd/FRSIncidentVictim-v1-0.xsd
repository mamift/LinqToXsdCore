	<xs:complexType name="VictimsStructure">
		<xs:annotation>
			<xs:documentation>Provides the structure for specifying victim information.  This provides a choice between entering full individual victim details, or just summary data.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="Victim" type="VictimStructure" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A Victim is involved in an Incident as a Fatality, Injury or just Rescued.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VictimSummary" type="VictimSummaryStructure">
				<xs:annotation>
					<xs:documentation>Summary information providing totals of all the types of victims involved in this special service incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="VictimStructure">
		<xs:annotation>
			<xs:documentation>Stores the information about a particular victim involved in an incident</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="VictimTypeType">
				<xs:annotation>
					<xs:documentation>9.6 - The type of this victim, fatality, casualty, or uninjured (rescued)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocationAtFireStart" type="LocationAtFireStartStructure">
				<xs:annotation>
					<xs:documentation>9.10 - The location of this victim when the fire initially started if known.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WhereFound" type="WhereFoundStructure">
				<xs:annotation>
					<xs:documentation>9.11 - Describes where this victim was found at the scene by the fire officers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Role" type="VictimRoleStructure">
				<xs:annotation>
					<xs:documentation>9.12 - The role played by this victim, eg resident, firefighter etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PersonName" type="pdt:PersonNameStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>9.18 and 9.19 - The Persons family name should be populated if available. It must be populated for Fatalities and for all Fire Injured victims with Serious SeverityOfInjury = 1.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Age">
				<xs:annotation>
					<xs:documentation>9.7 - Approximate age of the victim e.g. Child, Elderly, etc.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="fire:ZeroTo999Type">
						<xs:maxInclusive value="115"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Gender" type="pdt:GenderCurrentType">
				<xs:annotation>
					<xs:documentation>9.8 - Gender (census categories)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ethnicity" type="EthnicityStructure">
				<xs:annotation>
					<xs:documentation>9.9 - Ethnicity (census categories)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WasRescued" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>9.13 - Was this victim rescued at the scene, regardless of whether they sustained any injuries (Fatality, Injured or just Rescued).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RescueDetails" type="RescueDetailsStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If this victim WasRescued, then this section contains details on the rescue that took place. Must be populated for Rescued individuals. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>This choice structure is determined by the value of the victim type field.   Note: A Casualty is defined as a Fatality or Injury. </xs:documentation>
				</xs:annotation>
				<xs:element name="Fatality" type="FatalityStructure">
					<xs:annotation>
						<xs:documentation>Extra details only needed for fatality victims</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Injured" type="InjuredStructure">
					<xs:annotation>
						<xs:documentation>Extra details only needed for injury victims</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Rescued" type="RescuedStructure">
					<xs:annotation>
						<xs:documentation>Extra details only needed for uninjured victims</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Id" type="fire:StringRestrictedKeyMandatoryType"/>
	</xs:complexType>
	<xs:complexType name="FatalityStructure">
		<xs:annotation>
			<xs:documentation>Contains the information specific to a fatality victim</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Circumstances" type="FatalityCircumstancesType">
				<xs:annotation>
					<xs:documentation>9.20 - e.g. Thought to be already dead on arrival.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CauseOfDeath" type="CauseOfDeathStructure">
				<xs:annotation>
					<xs:documentation>9.22 - e.g. Burns or scalds, Heart Attack, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeathCertificateMarker" type="DeathCertificateMarkerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>9.23 - A status determining if the Death Certificate has been received and checked. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WasFireRelated" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>9.21 - Was the fataility specifically due to Fire?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OxygenProvided" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>9.26 - Was oxygen give to the casualty victim?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherCircumstances" type="OtherCircumstancesStructure">
				<xs:annotation>
					<xs:documentation>9.25 - e.g. Fell onto Fire, Trapped by Smoke, Injured by blast, etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InjuredStructure">
		<xs:annotation>
			<xs:documentation>Contains the information specific to a casualty victim</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NatureOfInjury" type="NatureOfInjuryStructure">
				<xs:annotation>
					<xs:documentation>9.22 - e.g. Internal injury, Drowning, Severe Burns, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeverityOfInjury" type="SeverityOfInjuryType">
				<xs:annotation>
					<xs:documentation>9.24 - Medical Assistance e.g. Serious â€“ overnight stay in hospital as in-patient.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherCircumstances" type="OtherCircumstancesStructure">
				<xs:annotation>
					<xs:documentation>9.25 - e.g. Fell onto Fire, Trapped by Smoke, Injured by blast, etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OxygenProvided" type="core:YesNoType">
				<xs:annotation>
					<xs:documentation>9.26 - Was oxygen give to the casualty victim?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WasFireRelated" type="fire:YesNoDontKnowType">
				<xs:annotation>
					<xs:documentation>9.21 - Was the injury specifically due to Fire?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RescuedStructure">
		<xs:annotation>
			<xs:documentation>Contains the information specific to an uninjured victim</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OtherCircumstances" type="OtherCircumstancesStructure">
				<xs:annotation>
					<xs:documentation>9.25 - e.g. Bedridden, Chair-ridden, Drunk, etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RescueDetailsStructure">
		<xs:annotation>
			<xs:documentation>Contains the information about whether or not this victim was rescued</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Location" type="RescueLocationType">
				<xs:annotation>
					<xs:documentation>9.14 - location and circumstances e.g. Different room, cabin or compartment on floor of origin.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="By" type="RescuedByType">
				<xs:annotation>
					<xs:documentation>9.15 - Who rescued the victim e.g. FRS personnel with breathing apparatus.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Method" type="RescueMethodStructure">
				<xs:annotation>
					<xs:documentation>9.16 - Rescue method e.g. Extension ladder</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="VictimTypeType">
		<xs:annotation>
			<xs:documentation>Provides an enumeration of the three distinct victim types (fatality, casualty/injury and uninjured/rescue)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Fatal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Injury (incl. rescue with injury)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Rescued (rescue without injury)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="VictimRoleStructure">
		<xs:annotation>
			<xs:documentation>Extends VictimRoleType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="VictimRoleType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="VictimRoleType">
		<xs:annotation>
			<xs:documentation>Lists the possible roles that a victim could have, eg firefighter</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Firefighter on duty</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Other emergency service personnel</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Resident/Occupant</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Passer by</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Driver</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Passenger</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Visitor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Employee in workplace</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation>Customer in shop</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation>Other FRS personnel on duty</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="LocationAtFireStartStructure">
		<xs:annotation>
			<xs:documentation>Extends LocationAtFireStartType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="LocationAtFireStartType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="LocationAtFireStartType">
		<xs:annotation>
			<xs:documentation>Defines the options available to describe the location of a victim when the fire started.  This adds the 'not known' option to the where found list.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="WhereFoundType fire:NotKnownType"/>
	</xs:simpleType>
	<xs:complexType name="WhereFoundStructure">
		<xs:annotation>
			<xs:documentation>Extends WhereFoundType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="WhereFoundType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="WhereFoundType">
		<xs:annotation>
			<xs:documentation>Defines the options available to describe where a victim was found</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Room, cabin or compartment of origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Different room, cabin or compartment on floor of origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Floor above origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Two or more floors above origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>One floor below origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Two or more floors below origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Outside building, vehicle etc of origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation>Seat of fire unknown or multi-seated (and above not applicable)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Location of person unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="98">
				<xs:annotation>
					<xs:documentation>Not applicable</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other Location</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RescuedByType">
		<xs:annotation>
			<xs:documentation>Defines the set of avilable options for the rescued by field</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Firefighter without breathing apparatus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Firefighter with breathing apparatus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Other without breathing apparatus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Other with breathing apparatus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Rescuer not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="RescueMethodStructure">
		<xs:annotation>
			<xs:documentation>Extends RescueMethodType to provide the 'other' attribute so that the user's custom value can be stored.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="RescueMethodType">
				<xs:attributeGroup ref="fire:OtherAttributes"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="RescueMethodType">
		<xs:annotation>
			<xs:documentation>Defines the set of available options for the resuce methods used field</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Ladder</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Aerial appliance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation>Specialist rope rescue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Rescue with fire service lines e.g. lowered from height</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="12">
				<xs:annotation>
					<xs:documentation>Boat</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Supported/Carried out</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation>Extricated</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation>Released</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="13">
				<xs:annotation>
					<xs:documentation>Other organisationâ€™s resources e.g. helicopter, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Other method</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Not known</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RescueLocationType">
		<xs:annotation>
			<xs:documentation>Defines the available options for specifying the location and circumstances of the rescue</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Room, cabin or compartment of origin</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Different room, cabin or compartment on floor of origin </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Floor above origin - includes mezzanine above floor of origin</xs:documentation>
