         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:attributeGroup name="Violation.attlist">
      <xs:attributeGroup ref="lrml:commonLRMLNodeInit.attlist"/>
   </xs:attributeGroup>
