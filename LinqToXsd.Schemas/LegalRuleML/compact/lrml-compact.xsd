            <xs:group ref="ruleml:And-fo-node.choice"/>
            <xs:group ref="ruleml:Or-fo-node.choice"/>
            <xs:group ref="ruleml:Negation-fo-node.choice"/>
            <xs:group ref="ruleml:Implication-fo-node.choice"/>
